{
  "author": "WOPR",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.6",
    "@wopr-network/plugin-types": "^0.1.0",
    "discord.js": "^14.14.1",
    "prism-media": "^1.3.5",
    "sodium-native": "^5.0.10",
    "winston": "^3.19.0"
  },
  "description": "Discord voice channel integration for WOPR - Enables voice conversations in Discord",
